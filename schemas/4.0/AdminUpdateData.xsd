<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://ts.fujitsu.com/secdocs/v4_0/adminUpdateData"
    elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:tns="http://ts.fujitsu.com/secdocs/v4_0/adminUpdateData"
    xmlns:Q1="http://ts.fujitsu.com/secdocs/v4_0/adminData">
