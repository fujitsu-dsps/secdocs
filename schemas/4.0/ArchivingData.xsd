<!-- ================================================================= -->
    
    <xs:complexType name="TEvidenceRecord">
        <xs:annotation>
            <xs:documentation>SecDocs Evidence Record Information</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="TSPName"                      type="TNonEmptyString"        minOccurs="1" maxOccurs="1"/>
            <xs:element name="ERData"                       type="xs:base64Binary"        minOccurs="1" maxOccurs="1"/>
            <xs:element name="ERVerificationStatus"         type="TERVerificationStatus"  minOccurs="1" maxOccurs="1"/>
            <xs:element name="ERHtmlVerificationProtocol"   type="xs:base64Binary"        minOccurs="0" maxOccurs="1"/>
            <xs:element name="ERXmlVerificationProtocol"    type="xs:base64Binary"        minOccurs="1" maxOccurs="1"/>
            <xs:element name="HDOHashAlgorithmName"         type="TNonEmptyString"        minOccurs="1" maxOccurs="1"/>
            <xs:element name="HDOHashAlgorithmOID"          type="TNonEmptyString"        minOccurs="1" maxOccurs="1"/>
            <xs:element name="HDOHashValue"                 type="THDOHashValue"          minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="TNavigateQueryOptions">
        <xs:annotation>
            <xs:documentation>SecDocs WS operation navigate query types</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="subPaths"   type="xs:boolean" default="true"  minOccurs="0" maxOccurs="1"/>
            <xs:element name="aoids"      type="xs:boolean" default="true"  minOccurs="0" maxOccurs="1"/>
            <xs:element name="subjects"   type="xs:boolean" default="true"  minOccurs="0" maxOccurs="1"/>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="TNavigateSubpaths">
        <xs:annotation>
            <xs:documentation>list of subpaths in current path</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="subpath" type="xs:string" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>SecDocs navigate subpath name</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TNavigateAOIDs">
        <xs:annotation>
            <xs:documentation>list of AOIDs in current path</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="aoid" type="TNavigateAOIDItem" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>SecDocs navigate AOID</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="TNavigateAOIDItem">
        <xs:annotation>
            <xs:documentation>AOID entry for WS operation navigate</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="subject" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>SecDocs navigateSDO subject for item of type AOID</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="TUuid"/>
        <xs:attribute name="coid" use="optional" type="TCoid"/>
    </xs:complexType>
    
    <xs:complexType name="TSubSchemas">
        <xs:annotation>
            <xs:documentation>SecDocs schema data for a given AOID</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="subSchema" type="TSubSchema" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="TSubSchema">
        <xs:annotation>
            <xs:documentation>SecDocs schema data for a given AOID</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="location"  type="xs:string"       minOccurs="0" maxOccurs="1"/>
            <xs:element name="namespace" type="xs:string"       minOccurs="0" maxOccurs="1"/>
            <xs:element name="data"      type="xs:base64Binary" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TSimpleQueryKVPair">
        <xs:annotation>
            <xs:documentation>SecDocs simpleQuery key/value pair</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="key"   type="xs:string" minOccurs="1" maxOccurs="1"/>
            <xs:element name="value" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:annotation>
                        <xs:documentation>
                        The value can be a string representation of a given set of types.
                        If the value isn't a string, you can name the desired type via the 
                        type attribute.
                        </xs:documentation>
                    </xs:annotation>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute name="type" use="optional" default="string">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="string"/>
                                        <xs:enumeration value="integer"/>
                                        <xs:enumeration value="decimal"/>
                                        <xs:enumeration value="date"/>
                                        <xs:enumeration value="boolean"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="operation" type="TSimpleQueryKVPairOperation" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TSimpleQueryResult">
        <xs:annotation>
            <xs:documentation>SecDocs simpleQuery result</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="aoid"   type="TUuid"              minOccurs="1" maxOccurs="1"/>
            <xs:element name="kvPair" type="TSimpleQueryKVPair" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="THDOHashValue">
        <xs:annotation>
            <xs:documentation>SecDocs Hash Value Information</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="HashValue"    type="xs:base64Binary" minOccurs="1" maxOccurs="1"/>
            <xs:element name="HashValueHex" type="xs:string"       minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="TUuid">
        <xs:restriction base="xs:string">
            <xs:pattern value="[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:complexType name="TEmptyElement">
        <xs:annotation>
            <xs:documentation>XXXX</xs:documentation>
        </xs:annotation>
    </xs:complexType>

    <xs:simpleType name="TNonEmptyString">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"></xs:minLength>
            <xs:whiteSpace value="collapse"></xs:whiteSpace>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="TCoid">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"></xs:minLength>
            <xs:maxLength value="256"></xs:maxLength>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TSubmitSdoResponseStatus">
        <xs:restriction base="xs:string">
            <xs:enumeration value="SDO submitted successfully"/>
            <xs:enumeration value="SDO already submitted"/>
            <xs:enumeration value="SDO already submitted and sealed"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:complexType name="TMetaDataValue">
        <xs:annotation>
            <xs:documentation>meta data value</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name"         type="xs:string"  minOccurs="1" maxOccurs="1"/>
            <xs:element name="value"        type="xs:string"  minOccurs="0" maxOccurs="1"/>
            <xs:element name="type"         type="xs:string"  minOccurs="0" maxOccurs="1"/>
            <xs:element name="xpath"        type="xs:string"  minOccurs="0" maxOccurs="1"/>
            <xs:element name="defaultValue" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="TSubMapMetaData">
        <xs:annotation>
            <xs:documentation>sub container map meta data</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="metaDataValue" type="TMetaDataValue"  minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="subMap"        type="TSubMapMetaData" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="xpath" use="optional" type="xs:string"/>
    </xs:complexType>
    
    <xs:simpleType name="TERVerificationStatus">
        <xs:restriction base="xs:string">
            <!-- successful verification -->
            <xs:enumeration value="SUCCESS" />
            <!-- verification with warnings -->
            <xs:enumeration value="WARN" />
            <!-- verification failed -->
            <xs:enumeration value="FAIL" />
            <!-- verification result unknown -->
            <xs:enumeration value="UNDEFINED" />
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="TNavigateSDOItemType">
        <xs:restriction base="xs:string">
            <!-- successful verification -->
            <xs:enumeration value="path" />
            <!-- verification with warnings -->
            <xs:enumeration value="aoid" />
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="TSimpleQueryKVPairOperation">
        <xs:annotation>
            <xs:documentation>
            By default only values are found that are equal to the given value.
            With this operation you can change this behavior.
            Exception: the string value default behavior is contains.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="equal"/>
            <xs:enumeration value="eq"/>
            <xs:enumeration value="not-equal"/>
            <xs:enumeration value="ne"/>
            <xs:enumeration value="greater-than"/>
            <xs:enumeration value="gt"/>
            <xs:enumeration value="greater-equal"/>
            <xs:enumeration value="ge"/>
            <xs:enumeration value="less-than"/>
            <xs:enumeration value="lt"/>
            <xs:enumeration value="less-equal"/>
            <xs:enumeration value="le"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:complexType name="TListSdoVersionsRequest">
        <xs:annotation>
            <xs:documentation>SecDocs SOAP body request data for the WS operation listSdoVersions</xs:documentation>
        </xs:annotation>
    </xs:complexType>

