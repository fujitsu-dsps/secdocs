            <xs:element name="maxResults"   type="xs:unsignedInt"        minOccurs="0" maxOccurs="1" default="0"/>
            <xs:element name="start"        type="xs:string"             minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="useDatabase" use="optional" default="false" type="xs:boolean" />
    </xs:complexType>

