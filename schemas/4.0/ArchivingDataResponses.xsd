    <xs:import namespace="http://ts.fujitsu.com/secdocs/v4_0/secdocs" schemaLocation="secdocs.xsd"/>
    <xs:include schemaLocation="ArchivingData.xsd"/>
    <xs:import namespace="http://www.bsi.bund.de/tr-esor/xaip/1.2" schemaLocation="schemas/XAIP/1.2/tr-esor-xaip-v1.2.xsd"/>

    <xs:element name="getAoidResponse" type="TGetAoidResponse">
